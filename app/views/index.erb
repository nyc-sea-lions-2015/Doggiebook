    <div class="intro-header">
      <div class="container">
        <div class="row">
          <div class="col-lg-12">
          </div>
        </div>

      </div>
    </div>


    <div class="container" style="margin-top: 10px">
      <% if current_user %>
      <div class="well">
        <% @neighbourhoods.each do |neighbourhood| %>
        <a href="/neighbourhoods/<%= neighbourhood.id%>"><%= neighbourhood.name %></a><br>
        <% end %>
      </div>
    </div>
    <% else %>
    <div class="well">
      <% @neighbourhoods.each do |neighbourhood| %>
      <a href="/authentication/signin"><%= neighbourhood.name %></a><br>
      <% end %>
    </div>
  </div>
  <% end %>

  <%= erb :'partials/_signin' %>

  <%= erb :'partials/_signup' %>


